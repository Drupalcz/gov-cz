{
  "author": "Petr Illek",
  "name": "copy-czech-design-system",
  "version": "0.0.56",
  "scripts": {
    "copy": "delete & npm run copy-components & npm run copy-fonts & npm run copy-icons",
    "delete": "del-cli assets/gov-design-system",
    "copy-components": "copyfiles -u 6 node_modules/@gov-design-system-ce/components/dist/collection/components/**/*.css assets/gov-design-system/gov-components",
    "copy-fonts": "copyfiles -u 4 node_modules/@gov-design-system-ce/fonts/lib/*.woff2 assets/gov-design-system/gov-fonts",
    "copy-icons": "copyfiles -u 4 node_modules/@gov-design-system-ce/icons/lib/**/*.svg assets/gov-design-system/gov-icons"
  },
  "dependencies": {
    "@gov-design-system-ce/components": "0.0.56",
    "@gov-design-system-ce/fonts": "0.0.2",
    "@gov-design-system-ce/icons": "0.0.9",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0"
  }
}
